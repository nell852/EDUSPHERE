{
  "expo": {
    "name": "EduSphere",
    "slug": "edusphere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.collegedeparis.edusphere"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.collegedeparis.edusphere"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-document-picker",
      "expo-file-system",
      "expo-image-picker",
      "expo-media-library",
      "expo-secure-store"
      
     
    ],
    "scheme": "edusphere",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
